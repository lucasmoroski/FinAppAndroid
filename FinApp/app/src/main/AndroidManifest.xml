<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.finapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FinApp">
        <activity android:name=".activity.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.DadosPesquisaActivity" />
        <activity android:name=".activity.PesquisarActivity" />
        <activity android:name=".activity.ListaClassificadaActivity" />
        <activity android:name=".activity.ExtratoActivity" />
        <activity android:name=".activity.CadastroActivity" />
        <activity android:name=".activity.MainActivity"/>
    </application>

</manifest>